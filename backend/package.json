{
  "name": "decolagem-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start:dev": "tsx watch src/server.ts",
    "build": "tsc --project . --skipLibCheck --noEmitOnError false --incremental false --sourceMap false",
    "copy-to-api": "if exist \"../api/_backend_dist\" rmdir /s /q \"../api/_backend_dist\" && mkdir \"../api/_backend_dist\" && xcopy /E /I /Y \"dist\\*\" \"../api\\_backend_dist\\\" && copy /Y \"dist\\server.js\" \"../api\\server.js\" 2>nul || echo 'Server.js not found, skipping copy'",
    "start": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "vercel-build": "tsc --project . --skipLibCheck --noEmitOnError false --incremental false --sourceMap false",
    "migrate:run": "tsx src/scripts/migrate.ts run",
    "migrate:create": "tsx src/scripts/migrate.ts create",
    "migration:run": "npm run migrate:run",
    "create-super-admin": "tsx src/scripts/createSuperAdmin.ts",
    "check-user-metadata": "tsx src/scripts/checkUserMetadata.ts",
    "update-user-role": "tsx src/scripts/updateUserRole.ts",
    "check-member-duplicates": "tsx src/scripts/checkMemberDuplicates.ts",
    "fix-member-auth-id": "tsx src/scripts/fixMemberAuthId.ts",
    "test-member-update": "tsx src/scripts/testMemberUpdate.ts",
    "test-member-update-auth": "tsx src/scripts/testMemberUpdateWithAuth.ts",
    "debug-member-update": "tsx src/scripts/debugMemberUpdate.ts",
    "apply-admin-rls": "tsx src/scripts/applyAdminRLSMigration.ts",
    "test-super-admin-update": "tsx src/scripts/testMemberUpdateWithSuperAdmin.ts",
    "check-rls-policies": "tsx src/scripts/checkRLSPolicies.ts",
    "debug-super-admin": "tsx src/scripts/debugSuperAdminPermissions.ts",
    "fix-super-admin-auth": "tsx src/scripts/fixSuperAdminAuth.ts",
    "test-register-validation": "tsx src/scripts/testRegisterValidation.ts",
    "create-member-entries": "tsx src/scripts/createMemberEntriesForSuperAdmins.ts",
    "seed": "echo 'Seeds devem ser executados manualmente no Supabase'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev:serve-frontend": "cross-env PORT=3001 SERVE_FRONTEND=true tsx watch src/server.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "@types/multer": "^2.0.0",
    "cors": "^2.8.5",
    "decolagem-system": "file:..",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.11.30",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}
