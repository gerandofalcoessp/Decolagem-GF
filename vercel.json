  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist",
        "outputDirectory": "frontend/dist",
        "nodeVersion": "22.x",
        "maxLambdaSize": "50mb"
      }
